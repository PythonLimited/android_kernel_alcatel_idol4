&pm8950_gpios {
	gpio@c400 { /* GPIO 5 */
		qcom,mode = <0>; /* Digital input */
		qcom,pull = <5>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <2>;
		qcom,master-en = <1>;
		status = "okay";
	};
};
